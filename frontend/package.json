{
  "name": "marker-exercise-frontend",
  "author": "Evert van Brussel",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p sync-types dev-only",
    "dev-only": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "sync-types": "nodemon -w .env generate-env-types.js",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@pinia/colada": "^0.6.0",
    "@tailwindcss/vite": "^4.0.0-alpha.15",
    "@types/lodash-es": "^4.17.12",
    "@vee-validate/zod": "^4.12.8",
    "@vueuse/core": "^10.10.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.379.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.8.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^4.0.0-alpha.15",
    "vee-validate": "^4.12.8",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@iconify-json/radix-icons": "^1.1.14",
